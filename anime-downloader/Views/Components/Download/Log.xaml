<UserControl x:Class="anime_downloader.Views.Components.Download.Log"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:download="clr-namespace:anime_downloader.ViewModels.Components.Download"
             d:DataContext="{d:DesignInstance download:LogViewModel}"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <TextBox Style="{StaticResource ConsoleBox}" Text="{Binding Text}" />
</UserControl>